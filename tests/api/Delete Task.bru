delete {
  url: {{baseUrl}}{{apiPrefix}}/tasks/{{taskId}}
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body.data.success: eq true
}

tests {
  test("should return 200 OK", function() {
    expect(res.getStatus()).to.equal(200);
  });

  test("should return success true", function() {
    expect(res.getBody().data.success).to.equal(true);
  });
}
